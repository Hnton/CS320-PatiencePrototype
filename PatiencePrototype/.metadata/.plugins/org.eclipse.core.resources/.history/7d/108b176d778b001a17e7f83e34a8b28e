public class Column extends Pile {

	/**
	 * NOT IMPLEMENTED
	 */
	@Override
	public boolean isFull() {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public boolean rules(PokerCard c) {
		
		if(pokerCards.size() == 0 || (pokerCards.get(0).getValue().ordinal() - 2 == c.getValue().ordinal() && pokerCards.get(0).getColor() != c.getColor()))
		{
			return true;
		}
	return false;
	}

	
}
